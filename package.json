{
    "name": "you-are-awesome-app-serverless-backend",
    "version": "0.5.0",
    "author": "Mirco Kraenz",
    "license": "UNLICENSED",
    "scripts": {
        "build": "tsc",
        "clean": "del-cli dist .build",
        "predeploy": "npm run clean",
        "deploy": "serverless deploy",
        "deploy:fn": "npm run clean && serverless deploy function -f",
        "deploy:test": "serverless offline",
        "deploy:test:verbose": "serverless offline --printOutput",
        "dev": "npm run deploy:test",
        "dev:prepare-db": "docker-compose up -d && ts-node scripts/setup-local-dynamodb.ts",
        "dev:prepare-db:hard-reset": "npm run dev:prepare-db -- --hard-reset",
        "test": "docker-compose up -d && jest",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
        "test:watch": "npm run test -- --watch"
    },
    "dependencies": {
        "@googleapis/sheets": "^0.3.0",
        "axios": "^0.27.2",
        "class-validator": "^0.13.2",
        "csvtojson": "^2.0.10",
        "expo-server-sdk": "^3.6.0",
        "lodash": "^4.17.21",
        "nodemailer": "^6.7.8",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.103",
        "@types/csvtojson": "^2.0.0",
        "@types/jest": "^26.0.23",
        "@types/jest-json-schema": "^2.1.3",
        "@types/lodash": "^4.14.184",
        "@types/mockdate": "^3.0.0",
        "@types/node": "12.x",
        "@types/nodemailer": "^6.4.2",
        "@types/uuid": "^8.3.1",
        "@types/validator": "^13.7.6",
        "aws-lambda": "^1.0.7",
        "aws-sdk": "^2.1210.0",
        "del-cli": "^3.0.1",
        "dotenv": "^10.0.0",
        "jest": "^26.6.3",
        "jest-json-schema": "^5.0.0",
        "mockdate": "^3.0.5",
        "prettier": "^2.3.1",
        "serverless": "^3.22.0",
        "serverless-iam-roles-per-function": "^3.2.0",
        "serverless-offline": "^8.8.1",
        "serverless-plugin-aws-alerts": "^1.7.5",
        "serverless-plugin-typescript": "^2.1.2",
        "serverless-s3-sync": "^3.0.0",
        "ts-jest": "^26.5.6",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}
